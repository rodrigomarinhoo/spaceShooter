package meujogo.Modelo;

import java.awt.Image;

public class Bomb {
	
	private Image image;
	private int x,y;
	private int largura,altura;
	private boolean isVisivel;
	
	private static final int LARGURA = 600;
	private static int VELOCIDADE = 2;
	
	public Bomb(int x, int y) {
		this.x = x;
		this.y = y;
	}
}
